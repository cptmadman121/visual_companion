<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="BrandPrimary">#6E56CF</Color>
  <Color x:Key="BrandPrimaryVariant">#8E78FF</Color>
  <Color x:Key="BrandAccent">#22D3EE</Color>
  <Color x:Key="Surface">#121217</Color>
  <Color x:Key="SurfaceAlt">#17171D</Color>
  <Color x:Key="OnSurface">#EEF1F5</Color>
  <Color x:Key="OnSurfaceDim">#B8BDC8</Color>
  <Color x:Key="Danger">#EF4444</Color>
  <Color x:Key="Success">#10B981</Color>

  <SolidColorBrush x:Key="BrushBrandPrimary" Color="{StaticResource BrandPrimary}" />
  <SolidColorBrush x:Key="BrushBrandPrimaryVariant" Color="{StaticResource BrandPrimaryVariant}" />
  <SolidColorBrush x:Key="BrushBrandAccent" Color="{StaticResource BrandAccent}" />
  <SolidColorBrush x:Key="BrushSurface" Color="{StaticResource Surface}" />
  <SolidColorBrush x:Key="BrushSurfaceAlt" Color="{StaticResource SurfaceAlt}" />
  <SolidColorBrush x:Key="BrushOnSurface" Color="{StaticResource OnSurface}" />
  <SolidColorBrush x:Key="BrushOnSurfaceDim" Color="{StaticResource OnSurfaceDim}" />
  <SolidColorBrush x:Key="BrushDanger" Color="{StaticResource Danger}" />
  <SolidColorBrush x:Key="BrushSuccess" Color="{StaticResource Success}" />
</ResourceDictionary>
